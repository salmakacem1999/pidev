<!DOCTYPE html>

<title>Compte</title>

{% block body %}
    <h1>Compte</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ compte.id }}</td>
            </tr>
            <tr>
                <th>TypeCompte</th>
                <td>{{ compte.idType }}</td>
            </tr>
            <tr>
                <th>DateCreation</th>
                <td>{{ compte.dateCreation ? compte.dateCreation|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateFermeture</th>
                <td>{{ compte.dateFermeture ? compte.dateFermeture|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Solde</th>
                <td>{{ compte.solde }}</td>
            </tr>
            <tr>
                <th>CinS1</th>
                <td>{{ compte.cinS1 }}</td>
            </tr>
            <tr>
                <th>CinS2</th>
                <td>{{ compte.cinS2 }}</td>
            </tr>
            <tr>
                <th>OtherDoc</th>
                <td>{{ compte.otherDoc }}</td>
            </tr>
            <tr>
                <th>MaxSolde</th>
                <td>{{ compte.maxSolde }}</td>
            </tr>
            <tr>
                <th>MinSolde</th>
                <td>{{ compte.minSolde }}</td>
            </tr>
            <tr>
                <th>RedSolde</th>
                <td>{{ compte.redSolde }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_compte_index') }}">back to list</a>

    <a href="{{ path('app_compte_edit', {'id': compte.id}) }}">edit</a>

    {{ include('compte/_delete_form.html.twig') }}
{% endblock %}
